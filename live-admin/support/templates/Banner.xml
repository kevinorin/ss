<Template xmlns:Component="Component://approach.im" xmlns:Render="Render://approach.im">
	<Component:Banner type="json">
	{
		"banners":
		{
			"id" : "_self_id",
            "type": "BannerType",
            "banner_img":"agentProfileImg",
            "link_to":"urlname",
            "prop0":"line0",
            "prop1":"line1",
            "prop2":"line2",
            "prop3":"line3",
            "prop4":"line4",
            "prop5":"line5",
            "prop6":"line6"
		}
	}
	</Component:Banner>
	<Render:Markup bind="Banner">
    <div class="Overlay">
        <img class="agentProfile" src="[@ agentProfileImg @]" />
        <span class="line0"> Waterfront Residence </span>
        <span class="line1"> Miami Beach, FL 3453 </span>
        <span class="line2"> <span>5</span>Bedrooms </span>
        <span class="line3"> <span>4.5</span>Bathrooms </span>
        <span class="line4"> <span>6.124</span>SQ FT </span>
        <hr noshade="noshade" />
        <span class="line5"> [@ BannerLine5 @] </span>
        <a href="[@ urlname @]">
            <button type="button" class="btn btn-default" title="Learn much more" >$7,995,000</button>
        </a>
    </div>
	</Render:Markup>
</Template>